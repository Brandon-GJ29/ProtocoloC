#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/communication-protocol/node_modules/touch/bin/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/communication-protocol/node_modules/touch/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/communication-protocol/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/node_modules:/mnt/c/Users/Brandon/Documents/node_modules:/mnt/c/Users/Brandon/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/communication-protocol/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/communication-protocol/node_modules/touch/bin/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/communication-protocol/node_modules/touch/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/communication-protocol/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/node_modules:/mnt/c/Users/Brandon/Documents/node_modules:/mnt/c/Users/Brandon/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Brandon/Documents/Noveno Semestre/Cripto/Proyecto/communication-protocol/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../touch/bin/nodetouch.js" "$@"
fi
